{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/site-icon.png",
      "dest": "/site-icon.png"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/family/(.*)",
      "dest": "/family/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
